@article{guedonPatternIdentificationCharacterization2013,
  title = {Pattern Identification and Characterization Reveal Permutations of Organs as a Key Genetically Controlled Property of Post-Meristematic Phyllotaxis},
  author = {Guédon, Yann and Refahi, Yassin and Besnard, Fabrice and Farcot, Etienne and Godin, Christophe and Vernoux, Teva},
