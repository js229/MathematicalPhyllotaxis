@article{okabeUnifiedRulePhyllotaxis2019,
  title = {The Unified Rule of Phyllotaxis Explaining Both Spiral and Non-Spiral Arrangements},
  author = {Okabe, Takuya and Ishida, Atsushi and Yoshimura, Jin},
  date = {2019-02},
  journaltitle = {Journal of the Royal Society Interface},
