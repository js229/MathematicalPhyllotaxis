@article{douadyFibonacciQuasisymmetricPhyllotaxis2016,
  title = {Fibonacci or Quasi-Symmetric Phyllotaxis. {{Part II}}: Botanical Observations},
  shorttitle = {Fibonacci or Quasi-Symmetric Phyllotaxis. {{Part II}}},
  author = {Douady, Stéphane and Golé, Christophe},
  date = {2016-12-31},
  journaltitle = {Acta Societatis Botanicorum Poloniae},
  volume = {85},
