@article{elomaaFlowerHeadsAsteraceae2018,
  title = {Flower Heads in {{Asteraceae}}—Recruitment of Conserved Developmental Regulators to Control the Flower-like Inflorescence Architecture},
  author = {Elomaa, Paula and Zhao, Yafei and Zhang, Teng},
  date = {2018-07-01},
  journaltitle = {Horticulture Research},
