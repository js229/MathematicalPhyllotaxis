  booktitle = {Alan {{Turing}}: {{His Work}} and {{Impact}}},
  author = {Swinton, Jonathan},
  editor = {Cooper, S. Barry and family=Leeuwen, given=J., prefix=van, useprefix=true},
  date = {2013},
  pages = {834--848},
  publisher = {Elsevier},
  abstract = {After completing \textbackslash emph\{The Chemical Basis of Morphogenesis\}, and at some point in last three years of his life, Turing produced the pen-and-ink pictures reproduced as Figures throughout this article and published here for the first time. When his editors came to publish the morphogenesis volume of the Collected Works in 1992 they struggled with the fragmentary nature of \textbackslash emph\{The Morphogen Theory of Phyllotaxis\} and \textbackslash emph\{Outline of the Development of the Daisy\} and these images, without captions or a clear place, were not included. But in fact they tell a fascinating story of Turing's quest to explain, among much else, the appearance of Fibonacci numbers in the natural world. They have considerable historical interest as very early graphics of computational biology, but the work they represent remained in advance of published theory for decades. It is one goal of this article to represent these Figures and to explain their origin in the context of Turing's phyllotactic theory. The other goal is to provide a partial guide to those aspects of the surviving fragmentary works which can be understood from this perspective.},
  keywords = {turing},
  file = {C\:\\Users\\jonat\\Zotero\\storage\\28P58JD9\\msol.pdf;C\:\\Users\\jonat\\Zotero\\storage\\94WBCSJ8\\Swinton - 2013 - Turing, morphogenesis, and Fibonacci phyllotaxis Life in Pictures(2).pdf;C\:\\Users\\jonat\\Zotero\\storage\\Y9JGCJQR\\Swinton - 2013 - Turing, morphogenesis, and Fibonacci phyllotaxis Life in Pictures.pdf}
