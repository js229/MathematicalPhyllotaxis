  file = {C\:\\Users\\jonat\\Zotero\\storage\\KTD4H75L\\41586_2019_1693_MOESM1_ESM.pdf;C\:\\Users\\jonat\\Zotero\\storage\\WN43TDNF\\Leebens-Mack et al. - 2019 - One thousand plant transcriptomes and the phylogen.pdf;C\:\\Users\\jonat\\Zotero\\storage\\EEMYEELY\\s41586-019-1693-2.html}
