@article{goodallEigenshapeAnalysisCutGrow1991,
  title = {Eigenshape {{Analysis}} of a {{Cut-Grow Mapping}} for {{Triangles}}, and Its {{Application}} to {{Phyllotaxis}} in {{Plants}}},
