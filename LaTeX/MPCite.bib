@article{straussPhyllotaxisGoldenAngle2020,
  title = {Phyllotaxis: Is the Golden Angle Optimal for Light Capture?},
  shorttitle = {Phyllotaxis},
  author = {Strauss, Sören and Lempe, Janne and Prusinkiewicz, Przemyslaw and Tsiantis, Miltos and Smith, Richard S.},
  date = {2020},
  journaltitle = {New Phytologist},
  volume = {225},
