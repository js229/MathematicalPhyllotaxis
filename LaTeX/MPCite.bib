  keywords = {cell complex,Computational modelling,FEM - Finite Element Method,L-systems,plant development,plant morphogenesis,Position based dynamics},
  file = {C:\Users\jonat\Zotero\storage\J7WN7KPB\Marconi and Wabnik - 2021 - Shaping the Organ A Biologist Guide to Quantitative Models of Plant Morphogenesis.pdf}
}

