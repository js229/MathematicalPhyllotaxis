@article{bull-herenuMechanicalForcesFloral2022,
  title = {Mechanical {{Forces}} in {{Floral Development}}},
  author = {Bull–Hereñu, Kester and family=Santos, given=Patricia, prefix=dos, useprefix=true and Toni, João Felipe Ginefra and El Ottra, Juliana Hanna Leite and Thaowetsuwan, Pakkapol and Jeiter, Julius and Ronse De Craene, Louis Philippe and Iwamoto, Akitoshi},
